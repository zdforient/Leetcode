class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int n = *max_element(nums.begin(), nums.end());
        int s = 0;
        int p = 0;
        for (int i = 0; i < nums.size(); i ++){
            p = s;
            s += nums[i];
            if (s < 0){
                s = 0;
                continue;
            }
            if (s > n){
                n = s;
            }
        }
        return n;
        
    }
};
