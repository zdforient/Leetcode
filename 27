class Solution:
    def removeElement(self, nums: 'List[int]', val: 'int') -> 'int':
        if not nums:
            return 0
        k = 0
        d = 0
        for i in range(len(nums)):
            if nums[i] != val:
                
                nums[d] = nums[i]
                d += 1
        return d
