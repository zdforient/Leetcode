class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maxProfit = 0;
        if(!prices.size())
            return 0;
        int minprice = prices[0];
        for (int i = 0; i < prices.size(); i ++){
          if(minprice > prices[i]){
            minprice = prices[i];
            continue;
          }
          if(prices[i] > minprice + maxProfit)
            maxProfit = prices[i] - minprice;
        }
        return maxProfit;
    }
};
