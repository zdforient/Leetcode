class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i in range(len(nums)):
            for j in range(i+1,len(nums)):
                if nums[i] + nums[j] == target:
                    return [i,j]
    def __init__(self):
        num = 1
nums = [2, 7, 11, 15]
target = 9
S = Solution()
Solution.twoSum(S,nums,target)
